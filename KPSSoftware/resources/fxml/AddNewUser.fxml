<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox id="home-hbox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="600.0" prefWidth="700.0" spacing="5.0" styleClass="home-hbox" stylesheets="@../css/style.css"
      xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controller.addNewUserController">
    <children>
        <VBox id="home-vbox" prefHeight="490.0" prefWidth="105.0" spacing="3.0" styleClass="home-vbox"
              stylesheets="@../css/style.css">
            <children>
                <AnchorPane prefHeight="76.0" prefWidth="106.0">
                    <children>
                        <ImageView fx:id="avatar" fitHeight="59.0" fitWidth="65.0" layoutX="26.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../img/user.png"/>
                            </image>
                        </ImageView>
                        <Label fx:id="userLable" alignment="CENTER" layoutY="61.0" prefHeight="26.0" prefWidth="110.0"
                               styleClass="user-lable" stylesheets="@../css/style.css" text="Manager Dipen"
                               textAlignment="CENTER" wrapText="true">
                            <font>
                                <Font name="System Bold" size="14.0"/>
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
                <Button id="ChangePassword" mnemonicParsing="false" onAction="#changeScenes" prefHeight="60.0"
                        prefWidth="110.0" styleClass="main-button" stylesheets="@../css/style.css"
                        text="Change Password" textAlignment="CENTER" textFill="WHITE" wrapText="true">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button id="ManageUser" mnemonicParsing="false" onAction="#changeScenes" prefHeight="60.0"
                        prefWidth="110.0" styleClass="main-button" stylesheets="@../css/style.css" text="Manage Users"
                        textAlignment="CENTER" textFill="WHITE" wrapText="true">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button id="AddNewUser" mnemonicParsing="false" onAction="#changeScenes" prefHeight="60.0"
                        prefWidth="110.0" styleClass="main-button" stylesheets="@../css/style.css" text="Add New User"
                        textAlignment="CENTER" textFill="WHITE" wrapText="true">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button id="logout" mnemonicParsing="false" onAction="#changeScenes" prefHeight="60.0" prefWidth="110.0"
                        styleClass="logout-button" stylesheets="@../css/style.css" text="Log Out" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
            </children>
        </VBox>
        <AnchorPane id="home-anchorpane" prefHeight="490.0" prefWidth="586.0" styleClass="home-anchorpane"
                    stylesheets="@../css/style.css">
            <children>
                <Label layoutX="141.0" layoutY="22.0" prefHeight="58.0" prefWidth="289.0" text="Add New User"
                       textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="40.0"/>
                    </font>
                </Label>
                <Label layoutX="127.0" layoutY="101.0" prefHeight="36.0" prefWidth="150.0" text="First Name:"
                       textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>
                <TextField fx:id="FirstNameTextfield" layoutX="293.0" layoutY="96.0" prefHeight="45.0" prefWidth="193.0"
                           promptText="First Name"/>
                <Label layoutX="127.0" layoutY="154.0" prefHeight="36.0" prefWidth="132.0" text="Last Name:"
                       textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>
                <TextField fx:id="LastNameTextfield" layoutX="293.0" layoutY="149.0" prefHeight="45.0" prefWidth="193.0"
                           promptText="Last Name"/>
                <Label layoutX="128.0" layoutY="211.0" prefHeight="36.0" prefWidth="125.0" text="Gender:">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>
                <CheckBox fx:id="femaleBox" layoutX="293.0" layoutY="211.0" prefHeight="36.0" prefWidth="150.0"
                          text="Female"/>
                <CheckBox fx:id="maleBox" layoutX="418.0" layoutY="211.0" prefHeight="36.0" prefWidth="101.0"
                          text="Male"/>
                <Label layoutX="131.0" layoutY="266.0" prefHeight="36.0" prefWidth="138.0" text="Email:"
                       textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>
                <TextField fx:id="emailTextfield" layoutX="293.0" layoutY="261.0" prefHeight="45.0" prefWidth="193.0"
                           promptText="Email Address"/>
                <Label layoutX="131.0" layoutY="313.0" prefHeight="45.0" prefWidth="132.0" text="Phone Number:"
                       textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>
                <TextField fx:id="phoneTextfield" layoutX="293.0" layoutY="313.0" prefHeight="45.0" prefWidth="193.0"
                           promptText="Phone Number"/>
                <Label layoutX="135.0" layoutY="373.0" prefHeight="36.0" prefWidth="110.0" text="Username:"
                       textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>
                <TextField fx:id="usernameTextfield" layoutX="293.0" layoutY="368.0" prefHeight="45.0" prefWidth="193.0"
                           promptText="User Name"/>
                <Label layoutX="138.0" layoutY="434.0" prefHeight="36.0" prefWidth="138.0" text="Permission Level:">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>
                <CheckBox fx:id="managerBox" layoutX="293.0" layoutY="434.0" prefHeight="36.0" prefWidth="150.0"
                          text="Manager"/>
                <CheckBox fx:id="clerkBox" layoutX="426.0" layoutY="434.0" prefHeight="36.0" prefWidth="101.0"
                          text="Clerk"/>

                <Button id="accept" layoutX="119.0" layoutY="508.0" mnemonicParsing="false" onAction="#handleButtons"
                        prefHeight="36.0" prefWidth="101.0" styleClass="logout-button" stylesheets="@../css/style.css"
                        text="Accept" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button id="discard" layoutX="367.0" layoutY="508.0" mnemonicParsing="false" onAction="#handleButtons"
                        prefHeight="36.0" prefWidth="101.0" styleClass="discard-button" stylesheets="@../css/style.css"
                        text="Discard" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button id="reset" layoutX="244.0" layoutY="508.0" mnemonicParsing="false" onAction="#handleButtons"
                        prefHeight="36.0" prefWidth="101.0" styleClass="main-button" stylesheets="@../css/style.css"
                        text="Reset" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</HBox>
