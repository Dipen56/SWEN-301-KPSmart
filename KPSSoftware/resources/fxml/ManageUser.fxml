<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<HBox id="home-hbox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="600.0" prefWidth="700.0" spacing="5.0" styleClass="home-hbox" stylesheets="@../css/style.css"
      xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controller.UserSettingController">
    <children>
        <VBox id="home-vbox" prefHeight="490.0" prefWidth="105.0" spacing="3.0" styleClass="home-vbox"
              stylesheets="@../css/style.css">
            <children>
                <AnchorPane prefHeight="76.0" prefWidth="106.0">
                    <children>
                        <ImageView fx:id="avatar" fitHeight="59.0" fitWidth="65.0" layoutX="26.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../img/user.png"/>
                            </image>
                        </ImageView>
                        <Label fx:id="userLable" alignment="CENTER" layoutY="61.0" prefHeight="26.0" prefWidth="110.0"
                               styleClass="user-lable" stylesheets="@../css/style.css" text="Manager Dipen"
                               textAlignment="CENTER" wrapText="true">
                            <font>
                                <Font name="System Bold" size="14.0"/>
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
                <Button id="ChangePassword" mnemonicParsing="false" onAction="#changeScenes" prefHeight="60.0"
                        prefWidth="110.0" styleClass="main-button" stylesheets="@../css/style.css"
                        text="Change Password" textAlignment="CENTER" textFill="WHITE" wrapText="true">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button id="ManageUser" mnemonicParsing="false" onAction="#changeScenes" prefHeight="60.0"
                        prefWidth="110.0" styleClass="main-button" stylesheets="@../css/style.css" text="Manage Users"
                        textAlignment="CENTER" textFill="WHITE" wrapText="true">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button id="AddNewUser" mnemonicParsing="false" onAction="#changeScenes" prefHeight="60.0"
                        prefWidth="110.0" styleClass="main-button" stylesheets="@../css/style.css" text="Add New User"
                        textAlignment="CENTER" textFill="WHITE" wrapText="true">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
                <Button id="logout" mnemonicParsing="false" onAction="#changeScenes" prefHeight="60.0" prefWidth="110.0"
                        styleClass="logout-button" stylesheets="@../css/style.css" text="Log Out" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
            </children>
        </VBox>
        <AnchorPane id="home-anchorpane" prefHeight="490.0" prefWidth="586.0" styleClass="home-anchorpane"
                    stylesheets="@../css/style.css">
            <children>
                <Label layoutX="144.0" layoutY="14.0" prefHeight="58.0" prefWidth="288.0" text="Manage Users">
                    <font>
                        <Font name="System Bold" size="40.0"/>
                    </font>
                </Label>
                <TableView fx:id="tableView" layoutX="1.0" layoutY="98.0" prefHeight="395.0" prefWidth="575.0"
                           GridPane.columnIndex="0" GridPane.rowIndex="1">
                    <columns>
                        <TableColumn prefWidth="80" text="Username"/>
                        <TableColumn fx:id="firstNameColumn" prefWidth="100" text="First Name"/>
                        <TableColumn prefWidth="80" text="Last Name"/>
                        <TableColumn prefWidth="99.0" text="Email"/>
                        <TableColumn prefWidth="105.0" text="Phone"/>
                        <TableColumn prefWidth="131.0" text="Role"/>
                        <TableColumn prefWidth="73.0" text="Delete">
                        </TableColumn>
                    </columns>
                    <sortOrder>
                        <fx:reference source="firstNameColumn"/>
                    </sortOrder>
                </TableView>
                <Button id="Return" layoutX="228.0" layoutY="507.0" mnemonicParsing="false" onAction="#handleButtons"
                        prefHeight="40.0" prefWidth="120.0" styleClass="discard-button" stylesheets="@../css/style.css"
                        text="Exit" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</HBox>
